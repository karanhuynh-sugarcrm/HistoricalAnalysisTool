<script type="text/javascript">
  $(document).ready(function() {
    
   $('.tablesorter').tablesorter({
    theme: 'bootstrap',
    widgets: ['zebra']

   });
       
    $(document).on('click', '#submit', function(e){
      var a=$(this);
      a.html("Loading...")
      });
  });
</script>


